{
    "name": "o0h/phpcon-2024-ws",
    "description": "PHP Conference Japan 2024で実施する「作って理解するComposer <クイックコース>」のレポジトリです",
    "type": "project",
    "require": {
        "php": "8.4.*"
    },
    "require-dev": {
        "bamarni/composer-bin-plugin": "^1.8"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "o0h",
            "email": "dev@o0h.in"
        }
    ],
    "scripts": {
        "post-install-cmd": ["@composer bin cs-fixer install --ansi"],
        "cs-fixer": "php-cs-fixer fix"
    },
    "config": {
        "allow-plugins": {
            "bamarni/composer-bin-plugin": true
        },
        "sort-packages": true
    }
}
